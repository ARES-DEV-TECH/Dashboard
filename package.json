{
  "name": "ares-dashboard",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "description": "ARES Dashboard - Application de gestion d'entreprise",
  "author": "ARES Team",
  "scripts": {
    "postinstall": "prisma generate",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "dev": "next dev --turbo",
    "dev:webpack": "next dev --webpack",
    "build": "next build",
    "lint": "next lint",
    "start": "next start",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed-realistic.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "next build",
    "electron:pack": "npm run electron:build && electron-builder",
    "electron:dist": "npm run electron:build && electron-builder --publish=never",
    "electron:dist-mac": "npm run electron:build && electron-builder --mac --publish=never"
  },
  "dependencies": {
