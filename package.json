{
  "name": "ares-dashboard",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "description": "ARES Dashboard - Application de gestion d'entreprise",
  "author": "ARES Team",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev --turbo",
    "dev:webpack": "next dev --webpack",
    "build": "next build",
    "lint": "next lint",
    "start": "next start",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed-realistic.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "next build",
    "electron:pack": "npm run electron:build && electron-builder",
    "electron:dist": "npm run electron:build && electron-builder --publish=never",
    "electron:dist-mac": "npm run electron:build && electron-builder --mac --publish=never"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^10.38.0",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/papaparse": "^5.3.16",
    "@types/pg": "^8.15.5",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.546.0",
    "next": "15.5.9",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.3.0",
    "sonner": "^2.0.7",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  }
}
